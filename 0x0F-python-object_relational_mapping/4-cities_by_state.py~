#!/usr/bin/python3

import MySQLdb
import sys

myuser, password, database, search = sys.argv[1:]

db = MySQLdb.connect(host="localhost", user=myuser,
                     passwd=password, db=database)

cur = db.cursor()

cur.execute("SELECT * FROM cities ORDER BY id")

rows = curl.fetchall()

for row in rows:
    print(row)
