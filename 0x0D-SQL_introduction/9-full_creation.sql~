-- Create a second table in hbtn_0c_0 called second_table
-- The second_table description:
--				id INT
--				name VARCHAR(256)
--				score INT

CREATE TABLE IF NOT EXISTS second_table (
id INT,
name = VARCHAR(256),
score INT);

INSERT INTO second_table
VALUES (id = 1, name = 'John', score = 10), (id = 2, name = 'Alex', score = 3),
(id = 3, name = 'Bob', score = 14), (id = 4, name = 'George', score = 8);
