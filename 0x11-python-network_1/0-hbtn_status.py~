#!/usr/bin/python3
import urllib.request
import shutil
import tempfile

with urllib.request.open('http://alx-intranet.hbtn.io/status') as resp:
    with tempfile.NameTempFile as temp_file:
        shutil.copyfileobject(resp, temp_file)
    with open(temp_file.name, 'r') as html:
        lines = html.readlines()
        for line in lines:
            print(line)
